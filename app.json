{
  "expo": {
    "name": "kelly-pool",
    "slug": "kelly-pool",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo192.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo192.png",
      "resizeMode": "contain",
      "backgroundColor": "#323241"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo192.png",
        "backgroundColor": "#323241"
      }
    },
    "web": {
      "favicon": "./assets/logo192.png"
    }
  }
}
